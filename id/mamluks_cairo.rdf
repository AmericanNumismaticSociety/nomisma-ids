    <foaf:Organization rdf:about="http://nomisma.org/id/mamluks_cairo">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:prefLabel xml:lang="en">Mamluk Sultanate of Egypt</skos:prefLabel>
        <skos:prefLabel xml:lang="ar">مملوكي</skos:prefLabel>
        <skos:altLabel xml:lang="en">Mamluks</skos:altLabel>
        <skos:definition xml:lang="en">The Mamluk Sultanate (Turkish: Memlük Sultanlığı, Arabic: سلطنة المماليك‎ Sulṭanat al-Mamālīk) was a medieval realm spanning Egypt, the Levant, and Hejaz. It lasted from the overthrow of the Ayyubid Dynasty until the Ottoman conquest of Egypt in 1517.  Its capital was Cairo.</skos:definition>
        <skos:exactMatch rdf:resource="http://dbpedia.org/resource/Mamluk_Sultanate_%28Cairo%29"/>
        <nmo:hasStartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">1250</nmo:hasStartDate>
        <nmo:hasEndDate rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">1517</nmo:hasEndDate>
        <dcterms:isPartOf rdf:resource="http://nomisma.org/id/islamic_numismatics"/>
    </foaf:Organization>
